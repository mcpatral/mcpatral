pipeline {
    agent any
     stages{
      stage('Hello'){
       steps {
         echo "Hello World"
         sleep 10
       }
    }
    stage('Build'){
       steps {
         echo "Hello World"
         sleep 5
       }
    }
    stage('test'){
       steps {
         echo "Hello World"
         sleep 10
       }
    }
    stage('Deploy'){
       steps {
         echo "Hello World"
         sleep 10
       }
    }
    
    }
    
    

}